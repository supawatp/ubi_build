pipeline {
  agent any
  stages {
    stage('cleanup') {
      steps {
        sh 'docker system prune -a --volumes --force'
      }
    }
    stage('build image') {
      steps {
        git clone https://github.com/supawatp/ubi_build.git
        sh 'docker build -t subi8-minimal:8.4-212-1 .'
      }
    }
  }
}
